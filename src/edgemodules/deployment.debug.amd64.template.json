{
  "$schema-template": "4.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "recupreturnboxacr": {
                "username": "$CONTAINER_REGISTRY_USERNAME_recupreturnboxacr",
                "password": "$CONTAINER_REGISTRY_PASSWORD_recupreturnboxacr",
                "address": "recupreturnboxacr.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "PredictModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.PredictModule}",
              "createOptions": {}
            }
          },
          "AzureVideoAnalyzerEdge": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/media/video-analyzer:1",
              "createOptions": {
                "HostConfig": {
                  "LogConfig": {
                    "Type": "",
                    "Config": {
                      "max-size": "10m",
                      "max-file": "10"
                    }
                  },
                  "Binds": []
                }
              }
            }
          },
          "RtspSimModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.RtspSimModule}",
              "createOptions": {}
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "routes": {
          "PredictModuleToIoTHub": "FROM /messages/modules/PredictModule/outputs/* INTO $upstream",
          "AzureVideoAnalyzerEdgeToIoTHub": "FROM /messages/modules/AzureVideoAnalyzerEdge/outputs/* INTO $upstream",
          "RtspSimModuleToIoTHub": "FROM /messages/modules/RtspSimModule/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "AzureVideoAnalyzerEdge": {
      "properties.desired": {
        "ProvisioningToken": "eyJhbGciOiJSUzI1NiIsImtpZCI6InNmVkRoUzYtNkNLcjZnUkg2cDd0RWhxRUpNZyIsInR5cCI6IkpXVCJ9..PqKKzzdBaE0bRe1ilhvw8CsUt5tBIPPjHFmy7D6Vgd-MnrmObO8HLJryRcZOHss67KRrQBanA4IIjq-fmW7vlBkSxoSIsBvvss-cY1U10ryjjpnXjQpkk_bW5Dyrs303R2fK_Vg4VdT3iDj_nxQRadWHsQOfIiq5WjWtUCyhWLb7Cd10zT1xbqB2a5640abbmfXVS6P4b59nagnOhIHD29W6mMUUsRWnfqobMivjTFqKTUA8Pnx0fQFzNNPGcJbM0rZfUCqyNJ9aNAqmbxLN9htRBbJsCy960DiHfTO3UMoQQ3SR9UXCnAXpmLOEjTqCsE_jBWJj-nRxhqOiRj4ORg",
        "ApplicationDataDirectory": "/var/lib/videoanalyzer",
        "DiagnosticsEventsOutputName": "diagnostics",
        "OperationalEventsOutputName": "operational",
        "LogLevel": "Information",
        "LogCategories": "Application,Events",
        "AllowUnsecuredEndpoints": true,
        "TelemetryOptOut": false
      }
    }
  }
}